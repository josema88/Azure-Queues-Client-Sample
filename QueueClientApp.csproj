  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Azure.Storage.Queues" Version="12.3.2" />
  </ItemGroup>

  <ItemGroup>
    <None Remove="Assets\Messages.txt" />
  </ItemGroup>
  <ItemGroup>
    <Content Include="Assets\Messages.txt">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </Content>
  </ItemGroup>
</Project>
